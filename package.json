{
  "name": "express-bun-template",
  "version": "1.0.0",
  "description": "Express template running on Bun with TypeScript",
  "type": "module",
  "scripts": {
    "start": "bun index.ts",
    "dev": "bun --watch index.ts",
    "typecheck": "tsc --noEmit",
    "build": "tsc -p tsconfig.json --outDir dist"
  },
  "dependencies": {
    "express": "^4.18.2",
    "dotenv": "^16.0.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "typescript": "^5.1.6"
  }
}